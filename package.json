{
  "name": "ext-corb-workaround",
  "version": "2.0.0",
  "description": "A work-around for CORB restrictions in Chrome extensions",
  "main": "dist/src/index.js",
  "sideEffects": false,
  "scripts": {
    "prepare": "rimraf dist && babel -s true -d dist/src/ src/ -x .ts --ignore '**/*.test.ts' && tsc",
    "test": "npm run lint && jest",
    "lint": "eslint . --ext ts,tsx && prettier --check .",
    "lint-fix": "eslint . --ext ts,tsx --fix && prettier --write ."
  },
  "files": [
    "dist"
  ],
  "repository": "https://github.com/Macil/ext-corb-workaround",
  "author": "Chris Cowan <agentme49@gmail.com>",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "@babel/runtime": "^7.8.4",
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-typescript": "^7.8.3",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.14.149",
    "@types/node": "^20.11.17",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.5.0",
    "husky": "^4.2.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "3.2.5",
    "pretty-quick": "^4.0.0",
    "rimraf": "^5.0.5",
    "ts-node": "^10.4.0",
    "typescript": "^5.3.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
